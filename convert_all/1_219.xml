  <text>
    <body>
      <div1 n="1" type="volume">
        <div2 n="219" type="part">
          <head>219.</head>
          <lg type="sestet">
            <l n="1">Блок</l>
            <l n="2">Король</l>
            <l n="3">И маг порока;</l>
            <l n="4">Рок</l>
            <l n="5">И боль</l>
            <l n="6">Венчают Блока.</l>
          </lg>
          <date when="1911">10 декабря 1911</date>
        </div2>
      </div1>
    </body>
